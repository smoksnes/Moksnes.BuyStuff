{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "AzureStorage": "__Storage__",
    "AdoStorage": "Server=(localdb)\\mssqllocaldb;Database=OrleansDb;Trusted_Connection=True;MultipleActiveResultSets=True"
  },
  "SiloOptions": {
    "UseAzureStorage": false
  },
  "Kestrel": {
    "Limits": {
      "MaxConcurrentConnections": 100,
      "MaxConcurrentUpgradedConnections": 100
    },
    "DisableStringReuse": true
  }
}